.introduce-image{
  width: 100%;
  height: 150px;
}
view {
  padding: 10px;
}
.last-view {
  margin-bottom: 26px;
}
text {
  line-height: 2;
  font-size: 14px;
  font-weight: normal;
}
.introduce-title {
  font-size: 18px;
  font-weight: 400;
}
.introduce-address {
  color: #0366d6;
}
.introduce-address:hover{
  text-decoration: underline;
}
.time-item {
  padding: 0;
}
.time-item .date-text {
  width: 65px;
  display: inline-block;
}
.time-item text:nth-child(2) {
  margin-left: 10px;
}
.time-item text {
  font-size: 16px;
}

.submit-btn {
  position: fixed;
  bottom: 0;
}
.deep {
  background-color: #0652a8;
}