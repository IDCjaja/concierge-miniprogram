.introduce-image{
  width: 100%;
  height: 150px;
}
view {
  padding: 10px;
}
.last-view {
  margin-bottom: 46px;
}
text {
  line-height: 2;
  font-size: 14px;
  font-weight: normal;
}
.block-text {
  display: block;
}
.introduce-title {
  font-size: 18px;
  font-weight: 400;
}
.introduce-address {
  color: #0366d6;
}
.introduce-address:hover{
  text-decoration: underline;
}
.time-item {
  padding: 10px 0;
}
.time-item text:first-child {
  margin-left: 10px;
}
.time-item text {
  font-size: 16px;
}
.warning-text {
  font-size: 12px;
  color: red;
}
.deep {
  background-color: #0652a8;
}