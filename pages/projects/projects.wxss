.projects-contianer {
  height: 100%;
}
.projects-tabs {
  display: flex;
  justify-content: space-around;
  border-bottom: 1px solid #f1f1f1;
  padding-bottom:5px;
}
.tab {
  font-size: 16px;
}
.active {
  color: #409eff;
}
.current-projects {
  padding-top: 10px;
  background-color: #f1f1f1;
  margin-bottom: 10px;
}
.search-wrapper {
  border: 1px solid #ebeef5;
  border-radius: 5px;
  padding: 8px;
  margin:0 4%;
  display: flex;
  background-color: #f1f1f1;
}
.search-placeholder {
  color: #909399;
  margin-left: 5px;
}
.search-icon {
  display: flex;
  align-items: center;
}
.reservations-item {
  background-color: #fff;
  margin-top:10px;
  position: relative;
  padding:0 4%;
}
.reservations-item:first-child {
  margin-top: 0px;
}
.name-state-wrapper {
  font-size: 16px;
  display:flex;
  justify-content:space-between;
  padding: 13px 0;
}
.reservation-name {
  color: #222222;
}
.reservation-state {
  color: #000;
}
.reservation-datatime {
  margin-bottom: 10px;
  display: flex;
  align-items: center;
  min-height: 44px;
  border-bottom: 1px solid #edeef0;
}
.icon-datetime-wrapper {
  display: flex;
}
.date-time-item {
  font-size: 14px;
  color: #737373;
}
.icon {
  width: 15px;
  height: 15px;
  margin-top: 2px;
  margin-right: 15px;
}
.reservation-date{
  margin-right: 5px;
}
.reservation-address {
  padding-bottom: 10px;
  display: flex;
  position: relative;
}
.address-text {
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size: 14px;
  color: #737373;
}
.more-icon {
  position: absolute;
  right: 0;
  font-size: 14px;
  color: #737373;
}
.success {
  color: #06ecbe;
}
.waitting {
  color: #ffa449;
}
.history {
  color: #f1f1f1;
}