.projects-contianer {
  height: 100%;
}
.projects-tabs {
  display: flex;
  justify-content: space-around;
  border-bottom: 1px solid #f1f1f1;
  padding-bottom:5px;
}
.tab {
  font-size: 16px;
}
.active {
  color: blue;
}
.current-projects {
  padding-top: 10px;
  background-color: #f1f1f1;
  height: 100%;
  margin-bottom: 10px;
}
.search-wrapper {
  border: 1px solid #ebeef5;
  border-radius: 5px;
  padding: 8px;
  margin:0 3%;
  display: flex;
  background-color: #f1f1f1;
}
.search-placeholder {
  color: #909399;
  margin-left: 5px;
}
.search-icon {
  display: flex;
  align-items: center;
}
.reservations-item {
  background-color: #fff;
  margin-top:10px;
  position: relative;
  padding:10px 2%;
}
.reservations-item:first-child {
  margin-top: 0px;
}
.reservations-item view {
  line-height: 1.5;
}
.reservation-name {
  margin-bottom: 10px;
}
.reservation-datatime {
  margin-bottom: 10px;
  color: blue;
}
.reservation-date{
  margin-right: 5px;
}
.reservation-address {
  margin-bottom: 10px;
  color: blue;
}
.reservation-address:hover {
  text-decoration: underline;
}
.reservation-user {
  display:flex;
  justify-content:space-between;
}
.reservation-state {
  background-color:blue;
  width:50px;
  height:40px;
  border-radius:50%;
  display:flex;
  justify-content:center;
  align-items:center;
  color:#fff;
  position:absolute;
  top:5px;
  right:5px;
}