page {
  background-color: #f6f9fe;
}
.project {
  background-color:#fff;
  margin-top:15px;
  padding:4%;
  border: 1px solid #efeef0;
}
.project-cover {
  width:100%;
  height:200px;
  border-radius:4px;
  box-shadow: 0 0 10px rgba(0,0,0,0.3);
  margin-bottom: 5px;
}
.project-name {
  font-size: 16px;
  padding: 8px 0 10px 0;
  line-height: 1;
  font-weight: 400;
}
.project-address {
  color:#777;
}