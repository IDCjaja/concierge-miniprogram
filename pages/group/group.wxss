page {
  background-color: #f6f9fe;
}
.homepage-body {
  position: relative;
  height: 100%;
}
.group-title {
  padding: 10px 15px;
  font-size: 12px;
  color: #b3b3b3;
  line-height: 1;
}
.project {
  background-color:#fff;
  padding:4%;
  border-top: 1px solid #eceef0;
}
.project-cover {
  width:100%;
  height:200px;
  border-radius:4px;
  box-shadow: 0 0 10px rgba(0,0,0,0.3);
  margin-bottom: 5px;
}
.project-name {
  font-size: 16px;
  padding: 8px 0;
  line-height: 1;
  font-weight: 400;
}
.project-address {
  color:#777;
}
.no-result-wrap {
  position: absolute;
  width: 100%;
}
.no-groupProjects {
  display:flex;
  margin-top:82px;
  flex-direction:column;
  align-items:center;
}
.no-groupProjects-icon {
  width: 73px;
  height: 73px;
  margin-left: 12px;
}
.no-groupProjects-tip-top {
  font-size: 18px;
  color: #222222;
  margin-top: 20px;
  text-align: center;
}
.no-result-footer {
  position: fixed;
  width: 100%;
  background: #fff;
  bottom: 0;
  border-top: 1px solid #ecebf1;
  border-bottom: 1px solid #ecebf1
}
.backtohome {
  margin: 5px 4%;
  background: #409eff;
  color: #fff;
}